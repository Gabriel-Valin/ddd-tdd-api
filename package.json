{
  "name": "crud-nimbus-method",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config dist/Server.js",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig-build.json && tsc-alias",
    "go:dev": "ts-node-dev --respawn --transpile-only -r dotenv/config -r tsconfig-paths/register --clear src/Server.ts",
    "husky:prepare": "husky install",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm t -- --watch",
    "test:staged": "npm t -- --findRelatedTests",
    "test:coverage": "npm t -- --coverage --detectOpenHandles --forceExit",
    "test:integration": "npm t -- --config ./jest.integration.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "git-commit-msg-linter": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "prisma": "^4.6.1",
    "tsc-alias": "^1.7.1"
  },
  "prisma": {
    "schema": "./src/Infra/Utils/Prisma/schema.prisma"
  }
}
